# Allwinner H616 quad core 512MB/1GB RAM SoC WiFi SPI USB-C
BOARD_NAME="MangoPi MQ-Quad"
BOARDFAMILY="sun50iw9"
BOARD_MAINTAINER="rymut"
BOOTCONFIG="mangopi_mq_quad_defconfig"
